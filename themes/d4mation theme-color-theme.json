	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"keyword.other.phpdoc",
				"comment keyword.other.type",
				"comment storage.modifier",
				"comment support.class",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#29fd2f"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder",
				"punctuation.definition.variable",
				"variable.language",
				"variable.other.object.js",
				"variable.other.readwrite.js",
			],
			"settings": {
				"foreground": "#fd8023"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
